msgstr "%H∶%M, %d %B %Y"

#. Translators: Time in 12h format
#: ../js/misc/util.js:220
msgid "%l∶%M %p"
msgstr "%l∶%M %p"

#. Translators: this is the word "Yesterday" followed by a
#. time string in 12h format. i.e. "Yesterday, 2:30 pm"
#: ../js/misc/util.js:226
#, no-c-format
