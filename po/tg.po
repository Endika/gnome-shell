msgid "Yesterday, %H∶%M"
msgstr "Дирӯз, %H∶%M"

#. Translators: this is the week day name followed by a time
#. string in 24h format. i.e. "Monday, 14:30" */
#: ../js/misc/util.js:203
msgid "%A, %H∶%M"
msgstr "%A, %H∶%M"

